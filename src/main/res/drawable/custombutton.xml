<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
<item android:state_pressed="true" >
    <shape android:shape="rectangle"  >
        <corners android:radius="3dip" />
        <stroke android:width="1dip" android:color="#85969e" />
        <gradient android:angle="-90" android:startColor="#666d70" android:endColor="#596c75"  />
    </shape>
</item>
<item android:state_focused="true">
    <shape android:shape="rectangle"  >
        <corners android:radius="3dip" />
        <stroke android:width="1dip" android:color="#85969e" />
        <solid android:color="#85969e"/>
    </shape>
</item>
<item >
    <shape android:shape="rectangle"  >
        <corners android:radius="3dip" />
        <stroke android:width="1dip" android:color="#85969e" />
        <gradient android:angle="-90" android:startColor="#b1c4cc" android:endColor="#727f85" />
    </shape>
</item>
</selector>